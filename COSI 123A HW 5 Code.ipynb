   "source": [
    "amino_acids_to_codons ={\n",
    "    \"Phe\":[\"TTT\",\"TTC\"],\n",
    "    \"Leu\":[\"TTA\",\"TTG\",\"CTT\",\"CTC\",\"CTA\",\"CTG\"],\n",
    "    \"Ile\":[\"ATT\",\"ATC\",\"ATA\"],\n",
    "    \"Met\":[\"ATG\"],\n",
