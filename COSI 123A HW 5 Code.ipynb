    "import numpy as np\n",
    "\n",
    "def val_split(df, split):\n",
    "    \n",
    "    X = df.to_numpy()\n",
    "    np.random.shuffle(X)\n",
    "    y = X.T[-1]\n",
    "    X = X.T[:-1]\n",
    "    X = X.T\n",
    "    \n",
    "    split = int(split*len(train_data))\n",
    "\n",
    "    X_train = X[:split]\n",
    "    y_train = y[:split]\n",
    "    \n",
    "    X_val = X[split:]\n",
